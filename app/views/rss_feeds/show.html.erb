<%= page_title "#{@rss_feed.name} Feed" %>
<p style="color: green"><%= notice %></p>

<%= render partial: "feeds/feed", locals: {feed: @rss_feed} %>

  <%= link_to "Refresh Feed", refresh_rss_feed_path(@rss_feed), class: "btn" %>

<div>
  <%= link_to "Edit this rss feed", edit_rss_feed_path(@rss_feed) %> |
  <%= link_to "Back to rss feeds", rss_feeds_path %>

  <%= button_to "Destroy this rss feed", @rss_feed, method: :delete %>
</div>
